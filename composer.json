{
	"name" : "slothsoft/blob",
	"description" : "Blob implementation of https://www.w3.org/TR/FileAPI/",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.4",
		"slothsoft/core" : "^1.13.1",
		"slothsoft/w3c" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~9.6.23"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Blob\\" : "src"
		},
		"files" : [
			"scripts/bootstrap.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"docs"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Blob\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.1.x-dev"
		}
	},
	"homepage" : "http://farah.slothsoft.net/modules/blob",
	"archive" : {
		"exclude" : [
			"/.jenkins/",
			"/docs/",
			"/phpdoc.xml",
			"/tests/",
			"/test-files/",
			"/phpunit.xml"
		]
	}
}