{
    "name": "slothsoft/blob",
    "description": "Blob implementation of https://www.w3.org/TR/FileAPI/",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "slothsoft/core": "dev-master",
        "slothsoft/w3c": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\Blob\\": "src/"
        },
        "files": [
            "src/autoload.php"
        ]
    },
    "keywords": [
        "farah-abstract",
        "slothsoft"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false,
    "minimum-stability": "dev",
    "prefer-stable": true
}
